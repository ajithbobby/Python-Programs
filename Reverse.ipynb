        "**Program to reverse a number**\n"
      ],
      "metadata": {
        "id": "2jlHxD_S_Knm"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "n=int(input(\"Enter the number\"))\n",
        "rev=0\n",
        "while(n>0):\n",
        "  dig=n%10\n",
        "  rev=rev*10+dig\n",
        "  n=n//10\n",
        "print(\"Reverse of the number:\",rev)  "
      ],
